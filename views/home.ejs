<%- include("header") -%> 

<h1>Home</h1>
<p><%= startingContent %> </p>

<% for(const post of posts) { %> 
    <div class="post-tile">
        <h1><%= post.title %> </h1>
        <p><%= post.content.substring(0,100)+" ... " %><a href="/posts/<%= post._id%> ">Read More</a> </p>
    </div>
<% } %> 

<%- include("footer") -%> 